<?xml version="1.0" encoding="UTF-8"?>
<site name="${project.name}" xmlns="http://maven.apache.org/SITE/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SITE/2.0.0 https://maven.apache.org/xsd/site-2.0.0.xsd">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>${maven-fluido-skin.version}</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
        </fluidoSkin>
    </custom>
    <body>
        <menu name="User guide">
            <item href="index.html" name="Overview" />
            <item href="oshi-core-java11/apidocs/" name="Javadocs" />
            <item href="FAQ.html" name="Frequently Asked Questions" />
            <item href="Performance.html" name="Performance Considerations" />
            <item href="Upgrading.html" name="Major Version Breaking Changes" />
            <item href="SampleOutput.html" name="Sample Output" />
        </menu>
        <menu name="GitHub">
            <item href="https://github.com/oshi/oshi" name="OSHI on GitHub" />
            <item href="https://github.com/oshi/oshi/issues" name="Issues" />
            <item href="https://github.com/oshi/oshi/releases" name="Releases" />
        </menu>
        <menu ref="reports" />
        <menu ref="parent" inherit="top" />
        <menu ref="modules" inherit="top" />
    </body>
</site>
